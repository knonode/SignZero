    "contract": {
        "name": "SignZero",
        "methods": [
            {
                "name": "createApplication",
                "args": [],
                "readonly": false,
                "returns": {
                    "type": "void"
                },
                "desc": "Creates the application (empty state)"
            },
            {
                "name": "initializePetition",
                "args": [
                    {
                        "type": "string",
                        "name": "title",
                        "desc": "- Petition title (becomes ASA name, max 32 chars)"
                    },
                    {
                        "type": "byte[]",
                        "name": "text",
                        "desc": "- Petition content (stored in box, max 32KB)"
                    },
                    {
                        "type": "uint64",
                        "name": "duration",
                        "desc": "- Duration in rounds (min 25,000)"
                    }
                ],
                "readonly": false,
                "returns": {
                    "type": "uint64"
                },
                "desc": "Initializes the petition with funding, title, text, and duration\nMust be called with a payment transaction in the same group"
            },
            {
                "name": "signPetition",
                "args": [],
                "readonly": false,
                "returns": {
                    "type": "void"
                },
                "desc": "Signs the petition (validates atomic group with ASA opt-in)\nMust be called as first txn in atomic group where second txn is ASA opt-in"
            },
            {
                "name": "extendPetition",
                "args": [
                    {
                        "type": "uint64",
                        "name": "newEndRound",
                        "desc": "- New end round (must be greater than current)"
                    }
                ],
                "readonly": false,
                "returns": {
                    "type": "void"
                },
                "desc": "Extends the petition duration (author only)"
            },
            {
                "name": "finalizePetition",
                "args": [],
                "readonly": false,
                "returns": {
                    "type": "void"
                },
                "desc": "Finalizes the petition (anyone can call after end round)\nRemoves ASA manager and sends remaining balance to caller"
            },
            {
                "name": "getPetitionInfo",
                "args": [],
                "readonly": true,
                "returns": {
                    "type": "(uint64,uint64,uint64,bool,bool)"
                },
                "desc": "Read-only: Get petition info"
            }
        ],
        "networks": {}
    },
    "bare_call_config": {}
