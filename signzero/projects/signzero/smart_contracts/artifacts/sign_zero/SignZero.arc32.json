    "contract": {
        "name": "SignZero",
        "methods": [
            {
                "name": "createApplication",
                "args": [],
                "readonly": false,
                "returns": {
                    "type": "void"
                },
                "desc": "Creates the application (empty state)"
            },
            {
                "name": "initialize",
                "args": [
                    {
                        "type": "string",
                        "name": "title",
                        "desc": "- Opinion title (becomes ASA name, max 32 chars)"
                    },
                    {
                        "type": "byte[]",
                        "name": "text",
                        "desc": "- Opinion content (stored in box, max 32KB)"
                    },
                    {
                        "type": "uint64",
                        "name": "duration",
                        "desc": "- Duration in rounds (min 25,000)"
                    },
                    {
                        "type": "byte[]",
                        "name": "opinionType",
                        "desc": "- Opinion type (exactly 32 bytes, right-padded with zeros)"
                    },
                    {
                        "type": "string",
                        "name": "url",
                        "desc": "- Optional author website (max 96 bytes)"
                    }
                ],
                "readonly": false,
                "returns": {
                    "type": "uint64"
                },
                "desc": "Initializes the opinion with funding, title, text, duration, type, and optional URL\nMust be called with a payment transaction in the same group"
            },
            {
                "name": "sign",
                "args": [],
                "readonly": false,
                "returns": {
                    "type": "void"
                },
                "desc": "Signs the opinion (validates atomic group with ASA opt-in)\nMust be called as first txn in atomic group where second txn is ASA opt-in"
            },
            {
                "name": "extend",
                "args": [
                    {
                        "type": "uint64",
                        "name": "newEndRound",
                        "desc": "- New end round (must be greater than current)"
                    }
                ],
                "readonly": false,
                "returns": {
                    "type": "void"
                },
                "desc": "Extends the opinion duration (author only)"
            },
            {
                "name": "finalize",
                "args": [],
                "readonly": false,
                "returns": {
                    "type": "void"
                },
                "desc": "Finalizes the opinion (anyone can call after end round)\nRemoves ASA manager and sends remaining balance to caller"
            },
            {
                "name": "getInfo",
                "args": [],
                "readonly": true,
                "returns": {
                    "type": "(uint64,uint64,uint64,bool,bool)"
                },
                "desc": "Read-only: Get opinion info"
            }
        ],
